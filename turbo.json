{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [],
  "pipeline": {
    "build": {
      "inputs": [
        "src/**",
        "package.json",
        "tsconfig.json",
        "tsconfig.json",
        "../../tsconfig.json",
        "!src/*.test.ts",
        "!src/*.test.tsx",
        "!src/*.spec.ts",
        "!src/*.spec.tsx"
      ],
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "tsc": {
      "inputs": [
        "src/**",
        "package.json",
        "tsconfig.json",
        "tsconfig.json",
        "../../tsconfig.json"
      ],
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "lint": {
      "inputs": [
        "src/**",
        "package.json",
        "tests/**",
        ".eslintrc.js",
        ".eslintignore",
        "../../.prettierrc"
      ],
      "outputs": []
    },
    "watch": {
      "cache": false,
      "persistent": true,
      "outputs": []
    }
  }
}
