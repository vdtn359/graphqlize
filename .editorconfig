# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true

indent_style = space
indent_size = 2

[*.{js, ts}]
charset = utf-8
indent_style = spac
indent_size = 2

[Makefile]
indent_style = space

[*.yml]
indent_style = space
indent_size = 2
